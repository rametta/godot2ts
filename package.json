{
  "name": "godot2ts",
  "description": "Generate Typescript Types based on GDScript Files",
  "type": "module",
  "version": "0.0.5",
  "bin": {
    "godot2ts": "./dist/index.cjs"
  },
  "scripts": {
    "build": "bun build src/index.ts --outfile dist/index.cjs --target node --format cjs --external typescript",
    "test": "vitest",
    "format": "biome check ./src ./tests --write",
    "test:cli": "tsx src/cli.ts"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/rametta/godot2ts.git"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.6",
    "@types/bun": "latest",
    "tsx": "^4.20.6",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "cmd-ts": "^0.14.3",
    "glob": "^11.0.3",
    "tree-sitter": "^0.25.0",
    "tree-sitter-gdscript": "^5.0.1"
  }
}